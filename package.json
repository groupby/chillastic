{
  "name": "gb-es-reindexer",
  "version": "0.0.1",
  "description": "Reindex multiple elasticsearch indices, save your progress, mutate your data in-flight.",
  "main": "index.js",
  "scripts": {
    "test": "nyc --require babel-register mocha",
    "build": "babel index.js -d build && babel config -d build/config && babel lib -d build/lib",
    "coverage": "nyc report --require babel-register --reporter=text-lcov | coveralls",
    "report": "nyc report --require babel-register --reporter=html"
  },
  "nyc": {
    "lines": 93,
    "branches": 70,
    "functions": 90,
    "statements": 93,
    "check-coverage": true,
    "exclude": [
      "lib/master.js",
      "test/master.spec.js",
      "test/testMutators/",
      "test/invalidMutators"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.3.5",
    "bunyan": "^1.8.0",
    "bunyan-prettystream": "^0.1.3",
    "coveralls": "^2.11.9",
    "elasticsearch": "^11.0.1",
    "ioredis": "^1.15.1",
    "lodash": "^4.11.1",
    "moment": "^2.13.0",
    "multi-progress": "^1.0.0",
    "node-progress-3": "https://github.com/crash83k/node-progress-3/archive/0.3.2.tar.gz",
    "nyc": "^6.4.0",
    "stdio": "^0.2.7"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.7.7",
    "babel-istanbul": "^0.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "istanbul": "^0.4.3",
    "mocha": "^2.4.5",
    "nyc": "^6.4.0"
  }
}
